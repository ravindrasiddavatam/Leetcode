class Solution {
    public int majorityElement(int[] nums) {

    Arrays.sort(nums);
    int start=0;
    int end = nums.length-1;
    while(start<=end){
        int mid= (start+end)/2;
        
         return nums[mid]; 
    }
        return -1;
    }

    public static void main(String[] args)
    {
        Solution s=new Solution();
        int[] nums={2,2,2,2,1,1};
        System.out.println(s.majorityElement(nums));
    }
}
